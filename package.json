{
  "name": "foodbutler",
  "version": "1.0.0",
  "description": "CS 160 Team Smores",
  "main": "dist/server.js",
  "scripts": {
    "start:server": "node $npm_package_main",
    "clean:dist": "rimraf dist",
    "clean:public": "rimraf public && yarn copyfiles -u 3 \"src/client/root/*\" \"./src/client/images/*\" \"public/\"",
    "build:client": "webpack --config src/client/webpack.config.js",
    "build:server": "tsc -p src/server/tsconfig.json",
    "watch:client": "nodemon --watch 'src/client/**/*' --ext tsx,html --exec 'npm-run-all clean:public build:client'",
    "watch:server": "nodemon --watch 'src/server/**/*' --ext ts --exec 'npm-run-all clean:dist build:server start:server'",
    "start:dev": "npm-run-all --parallel watch:* --print-label"
  },
  "repository": "git+https://github.com/zachary-costa/spring2020-cs160-team-smores.git",
  "bugs": {
    "url": "https://github.com/zachary-costa/spring2020-cs160-team-smores/issues"
  },
  "homepage": "https://github.com/zachary-costa/spring2020-cs160-team-smores#readme",
  "keywords": [
    "sjsu",
    "cs160",
    "food",
    "butler",
    "pantry"
  ],
  "contributors": [
    "Alvin Nguyen",
    "Zachary Costa",
    "Michael Chu",
    "Gricelda Tecun",
    "Hunter Wright"
  ],
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/lodash": "^4.14.149",
    "@types/mysql": "^2.15.9",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
